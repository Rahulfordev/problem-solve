// find to nagative and possative number
const arr = [-3, 5, -6, 8, 0, 79, 99];

for (let i = 0; i < arr.length; i++) {
  if (arr[i] === 0) {
    console.log(`arr[i]`);
  } else if (arr[i] > 0) {
    console.log(`possative number ${arr[i]}`);
  } else {
    console.log(`nagative number ${arr[i]}`);
  }
}
